{
  "name": "typedoc",
  "description": "Create api documentation for TypeScript projects.",
  "version": "1.0.0-dev.3",
  "homepage": "https://typedoc.org",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "typedoc": "bin/typedoc.js"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/TypeStrong/TypeDoc.git"
  },
  "bugs": {
    "url": "https://github.com/TypeStrong/TypeDoc/issues"
  },
  "engines": {
    "node": ">= 10.12.0"
  },
  "dependencies": {
    "@types/color": "^3.0.1",
    "color": "^3.1.2",
    "lunr": "^2.3.9",
    "marked": "1.1.1",
    "minimatch": "^3.0.0",
    "preact": "^10.4.8",
    "preact-render-to-string": "^5.1.10",
    "shelljs": "^0.8.4",
    "shiki": "^0.2.2"
  },
  "peerDependencies": {
    "typescript": ">=4.0.2"
  },
  "devDependencies": {
    "@types/lunr": "^2.3.3",
    "@types/marked": "^1.1.0",
    "@types/minimatch": "3.0.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.2",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "mocha": "^8.1.3",
    "nyc": "15.1.0",
    "prettier": "2.1.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "bin",
    "dist",
    "!dist/test",
    "static",
    "tasks",
    "LICENSE"
  ],
  "scripts": {
    "pretest": "node scripts/copy_test_files.js",
    "test": "nyc --reporter=html --reporter=text-summary mocha --timeout=10000 'dist/test/**/*.test.js'",
    "prerebuild_specs": "npm run pretest",
    "rebuild_specs": "node scripts/rebuild_specs.js",
    "build": "tsc --project .",
    "build_and_test": "npm run build && npm run test",
    "lint": "eslint . && prettier -c .",
    "prepublishOnly": "node scripts/set_strict.js false && npm run lint && npm run build_and_test",
    "postpublish": "node scripts/set_strict.js true",
    "prepare": "npm run build",
    "clean": "rm -rf node_modules package-lock.json lib coverage"
  },
  "keywords": [
    "typescript",
    "documentation",
    "generator"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ]
  }
}
